{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "üöÄ Ejecutar Servidor Django",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/run.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/proyec_imprenta/imprenta_tucan"
            }
        },
        {
            "name": "üêç Django Server Direct",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/proyec_imprenta/imprenta_tucan/manage.py",
            "args": ["runserver"],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/proyec_imprenta/imprenta_tucan",
            "python": "${workspaceFolder}/imprenta_tuc/Scripts/python.exe"
        }
    ]
}
